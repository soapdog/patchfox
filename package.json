{
  "name": "patchfox",
  "version": "2022.11.1",
  "repository": {
    "url": "https://github.com/soapdog/patchfox"
  },
  "main": "src/cli.js",
  "bin": {
    "patchfox": "src/cli.js"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node src/cli.js",
    "docs": "node scripts/copy-package-docs.js",
    "release-script": "node scripts/make-release.js",
    "build": "run-s builder:*",
    "builder:mac-arm64": "pkg . --target node18-macos-arm64 --output bin/mac-arm64/patchfox",
    "builder:mac-x64": "pkg . --target node18-macos-x64 --output bin/mac-x64/patchfox",
    "builder:linux-arm64": "pkg . --target node18-linux-arm64 --output bin/linux-arm64/patchfox",
    "builder:linux-x64": "pkg . --target node18-linux-x64 --output bin/linux-x64/patchfox",
    "builder:win-arm64": "pkg . --target node18-win-arm64 --output bin/win-arm64/patchfox",
    "builder:win-x64": "pkg . --target node18-win-x64 --output bin/win-x64/patchfox",
    "release": "run-s build release-script"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "node_modules/leveldown/**/*"
    ],
    "outputPath": "bin",
    "targets": [
      "node16-macos-arm64",
      "node16-macos-x64",
      "node16-linux-arm64",
      "node16-linux-x64",
      "node16-win-x64",
      "node16-win-arm64"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "body-parser": "^1.20.1",
    "commander": "^9.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-bearer-token": "^2.4.0",
    "express-xmlrpc": "^0.0.1",
    "fetch": "^1.1.0",
    "fs-extra": "^11.1.0",
    "jayson": "^4.0.0",
    "lodash": "^4.17.21",
    "lori": "^1.0.5",
    "mkdirp": "^1.0.4",
    "multiserver-address": "^1.0.1",
    "pull-file": "^1.1.0",
    "pull-stream": "^3.6.14",
    "query-string": "^8.1.0",
    "require-glob": "^4.1.0",
    "rimraf": "^3.0.2",
    "secret-stack": "^6.4.1",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^2.0.1",
    "ssb-caps": "^1.1.0",
    "ssb-config": "^3.4.6",
    "ssb-conn": "^6.0.1",
    "ssb-db2": "^6.3.1",
    "ssb-ebt": "^9.1.2",
    "ssb-friends": "^5.1.1",
    "ssb-http-auth-client": "^1.1.1",
    "ssb-http-invite-client": "^1.0.1",
    "ssb-invite-client": "^1.3.3",
    "ssb-keys": "^8.2.0",
    "ssb-lan": "^1.2.3",
    "ssb-markdown": "^6.0.7",
    "ssb-master": "^1.0.3",
    "ssb-query": "^2.4.5",
    "ssb-ref": "^2.16.0",
    "ssb-replicate": "^1.3.3",
    "ssb-replication-scheduler": "^3.0.0",
    "ssb-room-client": "^2.0.2",
    "ssb-search2": "^2.1.3",
    "ssb-threads": "^10.4.0",
    "ssb-uri2": "^2.4.1",
    "uuid": "^9.0.0"
  }
}
