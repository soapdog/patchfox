{
  "name": "patchfox",
  "version": "2022.11.1",
  "repository": {
    "url": "https://github.com/soapdog/patchfox"
  },
  "main": "src/cli.js",
  "bin": {
    "patchfox": "src/cli.js"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development node src/cli.js",
    "docs": "node scripts/copy-package-docs.js",
    "release-script": "node scripts/make-release.js",
    "build": "run-s builder:*",
    "builder:mac-arm64": "pkg . --target node18-macos-arm64 --output bin/mac-arm64/patchfox",
    "builder:mac-x64": "pkg . --target node18-macos-x64 --output bin/mac-x64/patchfox",
    "builder:linux-arm64": "pkg . --target node18-linux-arm64 --output bin/linux-arm64/patchfox",
    "builder:linux-x64": "pkg . --target node18-linux-x64 --output bin/linux-x64/patchfox",
    "builder:win-arm64": "pkg . --target node18-win-arm64 --output bin/win-arm64/patchfox",
    "builder:win-x64": "pkg . --target node18-win-x64 --output bin/win-x64/patchfox",
    "release": "run-s build release-script"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "node_modules/leveldown/**/*"
    ],
    "outputPath": "bin",
    "targets": [
      "node16-macos-arm64",
      "node16-macos-x64",
      "node16-linux-arm64",
      "node16-linux-x64",
      "node16-win-x64",
      "node16-win-arm64"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.8.0"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "body-parser": "^1.20.1",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-bearer-token": "^2.4.0",
    "fetch": "^1.1.0",
    "jayson": "^4.0.0",
    "lodash": "^4.17.21",
    "lori": "^1.0.5",
    "mkdirp": "^1.0.4",
    "pull-abortable": "^4.1.1",
    "pull-box-stream": "^1.0.13",
    "pull-cat": "^1.1.11",
    "pull-catch": "^1.0.1",
    "pull-defer": "^0.2.3",
    "pull-file-reader": "^1.0.2",
    "pull-identify-filetype": "^1.1.0",
    "pull-many": "^1.0.8",
    "pull-next": "^1.0.1",
    "pull-next-query": "^1.0.0",
    "pull-paginate": "^1.0.0",
    "pull-paramap": "^1.2.2",
    "pull-promise": "^2.0.0",
    "pull-reader": "^1.3.1",
    "pull-reconnect": "0.0.3",
    "pull-sort": "^1.0.2",
    "pull-split": "^0.2.0",
    "pull-stream": "^3.6.14",
    "pull-utf8-decoder": "^1.0.2",
    "query-string": "^6.8.2",
    "rss-parser": "^3.7.6",
    "scuttle-book": "^2.0.6",
    "scuttle-gathering": "^1.3.6",
    "secret-stack": "^6.4.1",
    "ssb-about": "^2.0.1",
    "ssb-avatar": "^0.2.0",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^2.0.1",
